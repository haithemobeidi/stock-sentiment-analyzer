{
  "name": "stock-sentiment-backend",
  "version": "1.0.0",
  "description": "Backend API for stock sentiment analysis",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "stock",
    "sentiment",
    "analysis",
    "vader"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "pg": "^8.11.3",
    "drizzle-orm": "^0.29.3",
    "node-cache": "^5.1.2",
    "axios": "^1.6.5",
    "vader-sentiment": "^1.1.3",
    "date-fns": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "drizzle-kit": "^0.20.10"
  }
}
